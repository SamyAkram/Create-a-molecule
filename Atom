public class Atom {

  //Attributes
  private String name;
  
  //Constructor
  public Atom(String aName){
    if (aName.equals("C")){
      name = "C"; //An atom of carbon is created
    } else if (aName.equals("H")){
      name = "H"; //An atom of hydrogen is created
    } else if (aName.equals("O")){
      name = "O"; //An atom of oxygen is created
    } else if (aName.equals("N")){
      name = "N"; //An atom of nitrogen is created
    } else if (aName.equals("Cl")){
      name = "Cl"; //An atom of chlorine is created
    } else if (aName.equals("F")){
      name = "F"; //An atom of fluorine is created
    } else if (aName.equals("S")){
      name = "S"; //An atom of sulfur is created
    } else if (aName.equals("Na")){
      name = "Na"; //An atom of sodium is created
    } else if (aName.equals("K")){
      name = "K"; //An atom of potassium is created
    } else if (aName.equals("P")){
      name = "P"; //An atom of phosphorus is created
    } else if (aName.equals("Fe")){
      name = "Fe"; //An atom of iron is created
    }
  }
  
  public String toString(){
    System.out.println("This is...");
  }

